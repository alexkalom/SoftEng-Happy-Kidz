<%- include partials/navbar.ejs %>


<div class="container well">
  <div class="row">



    <div class="tab" role="tabpanel">
      <!-- Nav tabs -->


      <ul class="nav nav-tabs " role="tablist">
        <li role="presentation" class="active" id="personal-tab" ><a href="#Section1" aria-controls="home" role="tab" data-toggle="tab"><span><i class="fa fa-user"></i></span>Λογαριασμοί Χρηστών</a></li>
        <li role="presentation" id="business-tab"><a href="#Section2" aria-controls="profile" role="tab" data-toggle="tab"><span><i class="fa fa-briefcase"></i></span>Λογαριασμοί Επιχειρήσεων</a></li>
        <li role="presentation"  id="event-tab" ><a href="#Section3" aria-controls="home" role="tab" data-toggle="tab"><span><i class="fa fa-bell"></i></span>Αιτήματα Εκδηλώσεων</a></li>
      </ul>

      <!-- Tab panes -->
      <div class="tab-content tabs">
        <div role="tabpanel" class="tab-pane fade in active" id="Section1">




          <div class="col-lg-12 my-form-personal">




            <div class="row">


              <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
                <thead>
                  <tr>
                    <th>Όνομα Χρήστη</th>
                    <th>Email Χρήστη</th>
                    <th class="no-sort">Απενεργοποίηση Λογαριασμού</th>
                    <th class="no-sort">Reset Κωδικού</th>

                      <!-- <th>Age</th>
                      <th>Start date</th>
                      <th>Salary</th> -->
                    </tr>
                  </thead>
                  <tbody>
                    <% parent.forEach(function(it) { %>
                    <tr>
                      <td><%= it.name %></td>
                      <td><%= it.email %></td>
                      <td>
                        <form role="form" action="/parent/<%=it.parentId%>?_method=DELETE" method="POST">
                          <input type="submit" class="btn btn-primary" name="delete" value="delete">
                        </form>
                      </td>
                      <td>
                        <form role="form" action="/admin/parent/<%=it.parentId%>/reset?_method=PUT" method="POST">
                          <input type="submit" class="btn btn-primary " name="reset" value="reset">
                        </form>
                      </td>

                      <!-- <td>61</td>
                      <td>2011/04/25</td>
                      <td>$320,800</td> -->
                    </tr>
                    <%});%>

                  </tbody>
                </table>
              </div>
            </div>
          </div>
          <div role="tabpanel" class="tab-pane fade" id="Section2">




            <div class="col-lg-12 my-form-business">




              <div class="row">
                <table id="example2" class="table table-striped table-bordered" cellspacing="0" width="100%">
                  <thead>
                    <tr>
                      <th>Όνομα Επιχείρησης</th>
                      <th>Κατάσταση Έγκρισης</th>
                      <th class="no-sort">Έγγραφο ΔΕΚΟ</th>
                      <th class="no-sort">Απενεργοποίηση Λογαριασμού</th>
                      <th class="no-sort">Reset Κωδικού</th>

                      <!-- <th>Age</th>
                      <th>Start date</th>
                      <th>Salary</th> -->
                    </tr>
                  </thead>
                  <tbody>
                    <% provider.forEach(function(it) { %>
                    <tr>
                      <td><a href="/admin/provider/<%=it.organizerId%>"><%= it.name %></a></td>
                      <td><%= it.isVerified%></td>
                      <td><a href="/provider/<%=it.organizerId%>/deko"> Έγγραφο ΔΕΚΟ%></td>
                        <td>
                          <form role="form" action="/provider/<%=it.organizerId%>?_method=DELETE" method="POST">
                            <input type="submit" class="btn btn-primary" name="delete" value="delete">
                          </form>
                        </td>
                        <td>
                          <form role="form" action="/admin/provider/<%=it.organizerId%>/reset?_method=PUT" method="POST">
                            <input type="submit" class="btn btn-primary " name="reset" value="reset">
                          </form>
                        </td>

                      <!-- <td>61</td>
                      <td>2011/04/25</td>
                      <td>$320,800</td> -->
                    </tr>
                    <%});%>

                  </tbody>
                </table>
              </div>
            </div>
          </div>
          <div role="tabpanel" class="tab-pane fade" id="Section3">




            <div class="col-lg-12 my-form-events">




              <div class="row">
                <table id="example3" class="table table-striped table-bordered" cellspacing="0" width="100%">
                  <thead>
                    <tr>
                      <th >Τίτλος Εκδήλωσης</th>
                      <th class="no-sort">Σελίδα Επιχείρησης</th>
                      <th>Ημερομηνία</th>

                      <!-- <th>Age</th>
                      <th>Start date</th>
                      <th>Salary</th> -->
                    </tr>
                  </thead>
                  <tbody>
                    <% event.forEach(function(it) { %>
                    <tr>
                      <td><a href="/admin/events/<%=it.eventId%>"><%= it.title %></a></td>
                      <td><a href="/admin/provider/<%=it.organizerId%>">Πατήστε Εδώ</a></td>
                      <td><% startTime = new Date(it.startTime*1000)%>
                        <%= startTime.getDate().toString() + "/" + (startTime.getMonth()+1).toString() %></td>
                      </tr>
                      <%});%>

                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>

      </body>


