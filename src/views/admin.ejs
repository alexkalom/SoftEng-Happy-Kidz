    $(document).ready(function() {
      $('#example').DataTable({order: [],columnDefs: [{ targets: 'no-sort', orderable: false }]});
    } );

    $(document).ready(function() {
      $('#example2').DataTable({order: [],columnDefs: [{ targets: 'no-sort', orderable: false }]});
    } );

    $(document).ready(function() {
